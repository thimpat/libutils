{
  "name": "@thimpat/libutils",
  "version": "1.0.0",
  "description": "Helper functions",
  "main": "cjs/index.cjs",
  "scripts": {
    "build:esm": "to-esm cjs/generic-functions.cjs --output esm/ --target esm --update-all",
    "build:minified": "to-esm cjs/generic-functions.cjs --target esm --bundle dist/test-utils.min.js --output esm/",
    "build:all": "npm run build:esm && npm run build:minified"
  },
  "author": "Patrice Thimothee",
  "license": "MIT",
  "devDependencies": {
    "eslinter": "^3.2.1",
    "to-esm": "^2.14.0"
  },
  "module": "./esm/generic-functions.mjs",
  "type": "module",
  "exports": {
    "require": "./cjs/generic-functions.cjs",
    "import": "./esm/generic-functions.mjs"
  }
}
